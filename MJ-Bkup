#!/bin/sh
find /home/sticks/Pictures/Midjourney/ -type f -name "*.png" -mtime +7 -exec rsync -av --remove-source-files {} /media/sticks/PNY/Midjourney/ \;
find /home/sticks/Pictures/Midjourney/ -type f -name "*.zip" -mtime +7 -exec rsync -av --remove-source-files {} /media/sticks/PNY/Midjourney/Zipfiles/ \;
find /home/sticks/Videos/ -type f -name "*.mp4" ! -name "VID*" -mtime +7 -exec rsync -av --remove-source-files {} /media/sticks/PNY/Videos/ \;
